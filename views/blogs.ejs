<% include ./partials/header%>
<div class="ui main container">
<h1>Blogs Page</h1>
<%blogs.forEach((blog)=>{%>
    <div class="ui segment">
        <h3><%=blog.name%></h3>
        <img style="width:100px" src="<%=blog.image%>" alt="almonds image">
        <p><%=blog.body%></p>
        <p><%=blog.created%></p>
        <a href="/blogs/<%=blog._id%>">Read More</a>

    </div>
<%});%>
</div>
<% include ./partials/footer%>
