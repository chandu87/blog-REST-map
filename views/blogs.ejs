<% include ./partials/header%>
<div class="ui container">
<h1>Blogs Page</h1>
<%blogs.forEach((blog)=>{%>
    <div>
        <h3><%=blog.name%></h3>
        <img style="width:300px" src="<%=blog.image%>" alt="almonds image">
        <p><%=blog.body%></p>
        <p><%=blog.created%></p>

    </div>
<%});%>
</div>
<% include ./partials/footer%>
